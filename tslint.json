{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier",
    "tslint-react-hooks"
  ],
  "rules": {
    "typedef": [false, "variable-declaration"],
    "object-literal-sort-keys": false,
    "interface-name": false,
    "jsx-no-lambda": false,
    "no-console": false,
    "variable-name": false
  }
}
